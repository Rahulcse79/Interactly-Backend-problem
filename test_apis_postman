curl --location --request POST 'http://localhost:4000/updateContact' \
--header 'Content-Type: application/json' \
--data-raw '{
    "contact_id": "3341363000000004001",
    "new_email": "rahulcse@009.com",
    "new_mobile_number": "431514354131",
    "data_store": "CRM"
}'

curl --location --request POST 'http://localhost:4000/createContact' \
--header 'Content-Type: application/json' \
--data-raw '{
    "first_name": "drupad",
    "last_name": "singh",
    "email": "rahulcse@009.com",
    "mobile_number": "431514354131",
    "data_store": "CRM"
}'
